FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    left
    {
        type fixedValue ;
        value uniform 0 ;
    }
    right
    {
        type fixedValue ;
        value uniform 0 ;
    }

    top   { type        zeroGradient; }
    bottom{ type        zeroGradient; }
    front {type empty;}
    back {type empty;}
}

